{
  "name": "sqltools-athena-driver",
  "displayName": "SQLTools AWS Athena Driver",
  "description": "SQLTools AWS Athena Driver",
  "version": "0.3.4",
  "publisher": "brunoventura",
  "license": "MIT",
  "icon": "icons/extension_icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/kovihq/sqltools-athena-driver"
  },
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters",
    "Other"
  ],
  "keywords": [
    "athena",
    "sqltools-driver",
    "sqltools",
    "driver"
  ],
  "activationEvents": [
    "*",
    "onLanguage:sql",
    "onCommand:sqltools.*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sqltools-athena-driver.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "@sqltools/base-driver": "^0.1.11",
    "@sqltools/types": "latest",
    "@aws-sdk/client-athena": "^3.100.0",
    "@aws-sdk/credential-providers": "^3.100.0",
    "@aws-sdk/types": "^3.100.0",
    "uuid": "^11.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.5.5",
    "@types/node": "22.x",
    "@types/vscode": "^1.84.0",
    "typescript": "^5.5.4"
  },
  "extensionDependencies": [
    "mtxr.sqltools"
  ]
}
